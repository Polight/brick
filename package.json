{
  "name": "lego",
  "version": "0.2.0",
  "description": "Lightweight future-proof web-components",
  "main": "index.js",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "mocha-jsdom": "^2.0.0"
  },
  "scripts": {
